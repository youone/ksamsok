<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF 
	xmlns="http://kulturarvsdata.se/ksamsok#" 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xml:base="http://kulturarvsdata.se/resurser/EntitySuperType">

	<EntitySuperType rdf:ID="agent">
		<name>Agent</name>
		<description>Person, grupp och organisation.</description>
	</EntitySuperType>

	<EntitySuperType rdf:ID="object">
		<name>Fysiskt ting</name>
		<description>Föremål, fotografi, bearbetningar/interaktiv resurs, film/video, ljud, dokument, karta, byggnad, fornlämning, konstverk, bok, samling, ritning, teckning och kulturmiljö.</description>
	</EntitySuperType>

	<EntitySuperType rdf:ID="event">
		<name>Händelse</name>
		<description>Avgränsas av tid och rum t.ex. historisk händelse och utställning.</description>
	</EntitySuperType>

	<EntitySuperType rdf:ID="concept">
		<name>Koncept</name>
		<description>En övergripande definition av ett event eller ett objekt t.ex. av krig.</description>
	</EntitySuperType>

	<EntitySuperType rdf:ID="information">
		<name>Informationsobjekt</name>
		<description>Immateriella objekt med identifierbar struktur, som t.ex. historier, berättelser, dikter, datamängder, källkod, mm.</description>
	</EntitySuperType>

</rdf:RDF>
